programa {
	funcao inicio() {
	    cadeia algo
		escreva("Digite uma das opções: álcool, gasolina, diesel ou fim. ")
		leia(algo)
		cadeia combustivel []= {"álcool", "gasolina", "diesel", "fim"}
		
		
		
		
		se(algo == combustivel[0]){
		    escreva(combustivel[0])
		}
		se(algo == combustivel[1]){
		    escreva(combustivel[1])
		}
		se(algo == combustivel[2]){
		    escreva(combustivel[2])
		}
		se(algo == combustivel[3]){
		    escreva(combustivel[3])
		}
		   
		}
		
		
	}